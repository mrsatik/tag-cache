{
  "name": "mrsatik/tag-cache",
  "description": "Tags cache with memcache",
  "minimum-stability": "stable",
  "keywords": [
    "tags",
    "cache",
    "memcache"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Perchenok Alexey",
      "email": "silenthill.sat@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "mrsatik/settings": "^1.2",
    "psr/simple-cache": "~1.0",
    "psr/cache": "~1.0",
    "ext-memcached": "*",
    "mrsatik/null-cache": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "mrsatik/cs": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "mrsatik\\TCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mrsatik\\TcacheTest\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
